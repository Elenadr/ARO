######################################################################
# /etc/network/interfaces -- configuration file for ifup(8), ifdown(8)

auto lo
iface lo inet loopback

auto eth0
iface eth0 inet static
     hwaddress ether 00:07:e9:00:04:00
     address 24.0.0.4
     netmask 255.255.255.0

auto eth1
iface eth1 inet static
     hwaddress ether 00:07:e9:00:04:01
     address 23.0.0.4
     netmask 255.255.255.0

auto eth2
iface eth2 inet static
     hwaddress ether 00:07:e9:00:04:02
     address 104.0.0.4
     netmask 255.255.255.0

auto eth3
iface eth3 inet static
     hwaddress ether 00:07:e9:00:04:03
     address 35.0.0.4
     netmask 255.255.255.0

auto eth4
iface eth4 inet static
     hwaddress ether 00:07:e9:00:04:04
     address 26.0.0.4
     netmask 255.255.255.0

up route add -net 101.0.0.0/24 gw 35.0.0.8

